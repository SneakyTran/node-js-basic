<!DOCTYPE html>
<html>

<head>
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        td,
        th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
</head>

<body>

    <h2>HTML Table</h2>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Firs tName</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Address</th>
            </tr>
        </thead>
        <tbody>
            <%for (let i=0; i < dataUsers.length; i++) { %>
                <tr>
                    <td>
                        <%=dataUsers[i].id%>
                    </td>
                    <td>
                        <%=dataUsers[i].firstName%>
                    </td>
                    <td>
                        <%=dataUsers[i].lastName%>
                    </td>
                    <td>
                        <%=dataUsers[i].email%>
                    </td>
                    <td>
                        <%=dataUsers[i].address%>
                    </td>
                    <td>
                        <a href="/detail/user/<%=dataUsers[i].id%>" target="_blank">Details</a>
                    </td>
                </tr>
            <%}%>
        </tbody>
    </table>

</body>

</html>